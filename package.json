{
  "name": "claudia-example-express-webapp",
  "version": "0.2.0",
  "description": "Example application for running a Node Express app on AWS Lambda using Amazon API Gateway.",
  "main": "lambda.js",
  "scripts": {
    "test": "mocha -R spec spec/spec.js",
    "deploy": "claudia create --handler lambda.handler --deploy-proxy-api --region us-east-1 --role claudia-express-executor",
    "update": "claudia update",
    "generate-proxy": "claudia generate-serverless-express-proxy --express-module app"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "aws-serverless-express": "^1.2.0",
    "express": "^4.14.0",
    "knox": "^0.9.2"
  },
  "devDependencies": {
    "claudia": "^2.1.0",
    "mocha": "^3.2.0",
    "should": "^11.1.1",
    "supertest": "^2.0.1"
  },
  "optionalDependencies": {
    "aws-sdk": "^2.7.8"
  }
}
